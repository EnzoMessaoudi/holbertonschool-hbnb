  +List_place()
}

class Places_Amenity{
    Places.int Id
    Amenity. int Id
}

class Amenity {
  +string name
  +string description
  +int Id
  +Create_amenity()
  +Update_amenity()
  +Delete_amenity()
  +List_amenity()

}
User --> UUID
Places --> UUID
Review --> UUID
Places "1" *-- "0..*" User
Review *--"1" User
Review "0..1" *-- "0..*" Places
Places "0..*" --> "0..*" Places_Amenity
Amenity "0..*" --> "0..*"  Places_Amenity
