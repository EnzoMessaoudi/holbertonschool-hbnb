classDiagram

class UUID{
    +User_creation_date
    +User_Update_date
    +Review_Creation_date
    +Review_update_date
    +Places_creation_date
    +Places_update_date
}

class User {
  +string first_name
  +string last_name
  -string email
  -string Password
  +int Id
  +write_review()
  +Create_acount()
  +Update_account()
  +delete_account()
}

class Review {
  +string text
  +int rating
  +int Id
  +update_text()
  +update_rating()
}

class Places {
  +string title
  +string description
  +float price
  +float latitude
  +float longitude
  +int Id
  +add_amenity()
  +remove_amenity()
